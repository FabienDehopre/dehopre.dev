{
  "mcpServers": {
    "nx": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "nx-mcp@latest"
      ],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "eslint": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@eslint/mcp@latest"
      ],
      "env": {}
    },
    "angular-cli": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@angular/cli@latest",
        "mcp"
      ],
      "env": {}
    }
  }
}